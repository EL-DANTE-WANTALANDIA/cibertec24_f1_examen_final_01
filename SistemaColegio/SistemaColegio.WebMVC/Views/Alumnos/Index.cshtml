@{
    ViewData["Title"] = "Alumnos";
}
<h1>@ViewData["Title"]</h1>
<table class="table">
    <tr>
        <th>Número</th>
        <th>Nombres</th>
        <th>Apellidos</th>
        <th>Dirección</th>
        <th>Sexo</th>
        <th>Fecha de Nacimiento</th>
    </tr>

    @foreach (var dato in ViewBag.ListaAlumnos)
    {
        <tr>
            <td>@dato.AlumnoID</td>
            <td>@dato.Nombres</td>
            <td>@dato.Apellidos</td>
            <td>@dato.Direccion</td>
            <td>@dato.Sexo</td>
            <td>@dato.FechaNacimiento</td>
        </tr>
    }
</table>
